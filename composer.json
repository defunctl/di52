{
  "name": "lucatume/di52",
  "description": "A PHP 5.2 compatible dependency injection container.",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Luca Tumedei",
      "email": "luca@theaveragedev.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6.20"
    }
  },
  "autoload": {
    "psr-4": {
      "lucatume\\DI52\\": "src/"
    },
    "files": [
      "aliases.php"
    ]
  },
  "require": {
    "php": ">=5.6",
    "psr/container": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  }
}
